{
  "name": "ts-retrofit2",
  "version": "2.0.16",
  "description": "A declarative and axios based retrofit implementation for JavaScript and TypeScript. (forked from https://github.com/nullcc/ts-retrofit)",
  "repository": "https://github.com/npwork/ts-retrofit2",
  "main": "dist/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "keywords": [
    "retrofit",
    "ts",
    "js",
    "axios"
  ],
  "bugs": "https://github.com/npwork/ts-retrofit2/issues",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc",
    "format": "prettier --config .prettierrc '{src,test}/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "make-badges": "istanbul-badges-readme",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint"
  },
  "author": "NP",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn lint && yarn make-badges && git add README.md"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "class-validator": "^0.12.2",
    "eslint-plugin-unused-imports": "^1.0.1",
    "form-data": "^3.0.0",
    "prettier": "^2.2.1",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/multer": "^1.3.10",
    "@types/qs": "^6.9.5",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "body-parser": "^1.19.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "istanbul-badges-readme": "^1.2.0",
    "jest": "^26.6.3",
    "multer": "^1.4.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
